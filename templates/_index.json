{# doxly.version #}
{# index.version #}
[{% for compound in index.compound %}
  {
    "kind": "{{ compound.kind }}",
    "refId": "{{ compound.refid }}",
    "template": "compound.md",
    "destination":"{% if compound.refid == 'indexpage' %}index{% else %}{{ compound.refid }}{% endif %}.md"
  }{% if not loop.last %},{% else %}
{% endif %}{% endfor %}]
